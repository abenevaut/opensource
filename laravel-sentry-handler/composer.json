{
    "name": "abenevaut/laravel-sentry-handler",
    "type": "library",
    "description": "Laravel sentry exceptions handler",
    "keywords": [
        "laravel",
        "sentry",
        "exceptions",
        "handler"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "me@abenevaut.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "sentry/sentry-laravel": "^2.12 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "laravel/framework": "^9.17 || ^10 || ^11",
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3.8",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\SentryHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
