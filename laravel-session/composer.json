{
    "name": "abenevaut/laravel-session",
    "type": "library",
    "description": "Laravel Session",
    "keywords": [
        "laravel",
        "session"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/session": "^9.19",
        "illuminate/broadcasting": "^9.52",
        "illuminate/http": "^9.52",
        "illuminate/queue": "^9.52",
        "laravel/framework": "^9.52"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phparkitect check",
            "vendor/bin/phpunit"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "mockery/mockery": "^1.5",
        "illuminate/database": "^9.44",
        "illuminate/events": "^9.44",
        "phparkitect/phparkitect": "^0.3.5",
        "squizlabs/php_codesniffer": "^3.11",
        "phpstan/phpstan": "^2.0"
    }
}
