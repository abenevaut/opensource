{
    "name": "abenevaut/echo-tracker",
    "description": "Is your socials communities growing? EchoTracker is a tool that helps you to track the number of followers.",
    "keywords": ["echo-tracker", "count", "followers"],
    "homepage": "https://github.com/abenevaut/opensource",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/abenevaut/opensource/issues",
        "source": "https://github.com/abenevaut/opensource"
    },
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "me@abenevaut.dev"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-bluesky-client"
        },
        {
            "type": "path",
            "url": "../laravel-discord-client"
        },
        {
            "type": "path",
            "url": "../laravel-instagram-client"
        },
        {
            "type": "path",
            "url": "../laravel-twitch-client"
        },
        {
            "type": "path",
            "url": "../laravel-x-client"
        }
    ],
    "require": {
        "php": "^8.3",
        "abenevaut/laravel-bluesky-client": "dev-main",
        "abenevaut/laravel-discord-client": "dev-main",
        "abenevaut/laravel-instagram-client": "dev-main",
        "abenevaut/laravel-twitch-client": "dev-main",
        "abenevaut/laravel-x-client": "dev-main",
        "illuminate/http": "^11.5",
        "laravel-zero/framework": "^11.36.1"
    },
    "require-dev": {
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/echo-tracker"]
}
