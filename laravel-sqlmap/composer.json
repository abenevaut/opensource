{
  "name": "abenevaut/laravel-sqlmap",
  "description": "A powerful package to automatically generate SQLMap collections from your Laravel routes with intelligent organization and rich documentation capabilities.",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^3.13"
  },
  "autoload": {
    "psr-4": {
      "abenevaut\\LaravelSqlMap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "abenevaut\\LaravelSqlMap\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Antoine Benevaut",
      "email": "antoine@benevaut.fr"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "abenevaut\\SqlMapServiceProvider\\PostmanServiceProvider"
      ]
    }
  },
  "license": "MIT"
}