{
    "name": "abenevaut/laravel-sqlmap",
    "description": "Laravel package to generate SQLMap files for SQL injection testing based on route reflection",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSqlMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSqlMap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSqlMap\\SqlMapServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

