{
    "name": "abenevaut/laravel-kite",
    "type": "library",
    "description": "Laravel Kite",
    "keywords": [
        "laravel",
        "kite"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "me@abenevaut.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/breeze": "^1.15|^2",
        "illuminate/auth": "^9.48|^10.20|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "phparkitect/phparkitect": "^0.3.5"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\Kite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phparkitect check",
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "abenevaut\\Kite\\App\\Providers\\KiteServiceProvider",
                "abenevaut\\Kite\\App\\Providers\\AuthServiceProvider",
                "abenevaut\\Kite\\App\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
