{
    "name": "abenevaut/laravel-ohdear",
    "type": "library",
    "description": "Laravel Ohdear API SDK",
    "keywords": [
        "laravel",
        "ohdear",
        "api",
        "sdk"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^9.27",
        "illuminate/routing": "^9.27",
        "guzzlehttp/guzzle": "^7.5",
        "spatie/data-transfer-object": "^3.9",
        "illuminate/validation": "^9.34",
        "illuminate/translation": "^9.34",
        "illuminate/pagination": "^9.34"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\Ohdear\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "laravel/framework": "^9.33",
        "fakerphp/faker": "^1.20"
    }
}
