{
    "name": "abenevaut/laravel-asana",
    "type": "library",
    "description": "Laravel Asana API SDK",
    "keywords": [
        "laravel",
        "Asana",
        "api",
        "sdk"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^9.27"
    },
    "autoload": {
        "psr-4": {
            "abenevaut\\Asana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpcs --standard=PSR12 ./src/Exceptions"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    }
}
